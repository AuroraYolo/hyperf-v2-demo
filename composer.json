{
  "name": "hyperf/biz-skeleton",
  "type": "project",
  "keywords": [
    "php",
    "swoole",
    "framework",
    "hyperf",
    "microservice",
    "middleware"
  ],
  "description": "A coroutine framework that focuses on hyperspeed and flexible, specifically use for build microservices and middlewares.",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "ext-redis": "*",
    "ext-swoole": ">=4.5",
    "hyperf/async-queue": "2.0.*",
    "hyperf/cache": "2.0.*",
    "hyperf/command": "2.0.*",
    "hyperf/config": "2.0.*",
    "hyperf/config-aliyun-acm": "^2.0",
    "hyperf/constants": "2.0.*",
    "hyperf/consul": "^2.0",
    "hyperf/contract": "2.0.*",
    "hyperf/database": "2.0.*",
    "hyperf/db-connection": "2.0.*",
    "hyperf/di": "2.0.*",
    "hyperf/dispatcher": "2.0.*",
    "hyperf/event": "2.0.*",
    "hyperf/exception-handler": "2.0.*",
    "hyperf/framework": "2.0.*",
    "hyperf/guzzle": "2.0.*",
    "hyperf/http-server": "2.0.*",
    "hyperf/json-rpc": "^2.0",
    "hyperf/logger": "2.0.*",
    "hyperf/metric": "^2.0",
    "hyperf/model-cache": "2.0.*",
    "hyperf/pool": "2.0.*",
    "hyperf/process": "2.0.*",
    "hyperf/redis": "2.0.*",
    "hyperf/retry": "^2.0",
    "hyperf/rpc-client": "^2.0",
    "hyperf/rpc-server": "^2.0",
    "hyperf/server": "2.0.*",
    "hyperf/service-governance": "^2.0",
    "hyperf/tracer": "^2.0",
    "hyperf/utils": "2.0.*",
    "jonahgeorge/jaeger-client-php": "^0.6.0",
    "overtrue/wechat": "~4.0",
    "symfony/property-access": "^5.1",
    "symfony/serializer": "^5.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14",
    "hyperf/devtool": "2.0.*",
    "hyperf/testing": "2.0.*",
    "mockery/mockery": "^1.0",
    "phpstan/phpstan": "^0.12.18",
    "swoole/ide-helper": "dev-master",
    "symfony/var-dumper": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Kernel/Functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "HyperfTest\\": "test/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": [],
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-autoload-dump": [
      "rm -rf runtime/container"
    ],
    "analyse": "phpstan analyse --memory-limit 300M -l 0 -c phpstan.neon ./app ./config",
    "cs-fix": "php-cs-fixer fix $1",
    "start": "php ./bin/hyperf.php start",
    "test": "co-phpunit -c phpunit.xml --colors=always"
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer"
    }
  }
}
